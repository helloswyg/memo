{
  "name": "nexty",
  "private": true,
  "_homepage": "https://helloswyg.github.io/memo/",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "distclean": "rm -rf .next out",
    "nodeclean": "rm -rf node_modules",
