    "deploy": "npm run distclean && npm run build && npm run export && touch ./out/.nojekyll && gh-pages -d out"
  },
  "dependencies": {
    "@swyg/corre": "^1.0.1",
    "@swyg/memo": "file:../",
    "next": "12.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@zeit/next-sass": "1.0.1",
    "eslint": "7",
    "eslint-config-next": "12.0.3",
    "gh-pages": "^3.2.3",
    "node-sass": "6.0.1"
  }
}
